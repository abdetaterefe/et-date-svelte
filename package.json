    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "sass": "^1.53.0",
    "svelte-preprocess": "^4.10.7"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/abdetaterefe/et-date-svelte.git"
  },
  "homepage": "https://github.com/abdetaterefe/et-date-svelte#readme",
